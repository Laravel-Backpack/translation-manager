{
    "name": "backpack/translation-manager",
    "description": "Translation Manager for Backpack",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Almeida",
            "email": "promatik@gmail.com",
            "homepage": "https://promatik.pt"
        },
        {
            "name": "Cristian Tabacitu",
            "email": "cristian.tabacitu@backpackforlaravel.com",
            "homepage": "https://backpackforlaravel.com"
        }
    ],
    "homepage": "https://github.com/backpack/translation-manager",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "TranslationManager"
    ],
    "require": {
        "backpack/crud": "dev-update-laravel as 6.7",
        "spatie/laravel-translation-loader": "dev-l12-compatibility as 2.8",
        "outhebox/blade-flags": "dev-l12-compatibility",
        "calebporzio/sushi": "^2.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        },  
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/gravatar.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/alerts.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-translation-loader.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/blade-flags.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~10.0|~11.0",
        "orchestra/testbench": "~8.0|~9.0|~10.0",
        "larastan/larastan": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\TranslationManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\TranslationManager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": [
            "vendor/bin/phpstan analyse"
        ],
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backpack\\TranslationManager\\AddonServiceProvider"
            ]
        }
    }
}
